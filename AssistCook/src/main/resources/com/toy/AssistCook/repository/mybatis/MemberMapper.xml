<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.toy.AssistCook.repository.mybatis.MemberMapper">
    <insert id="save" useGeneratedKeys="true" keyProperty="id">
        insert into member (login_id, pw, name, birth, address, email, profile, grade)
        values (#{login_id}, #{pw}, #{name}, #{birth}, #{address}, #{email}, #{profile}, #{grade})
    </insert>

    <select id="findById">
        select *
        from member
        where login_id = #{loginId}
    </select>

</mapper>